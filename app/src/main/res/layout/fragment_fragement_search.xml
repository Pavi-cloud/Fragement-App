<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/searchFragment"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".FragementSearch">

    <TextView
        android:id="@+id/attach"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="onAttach()"
        android:textSize="25dp"
        android:textStyle="bold"
        android:textColor="@color/black"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="30dp"
        android:layout_marginLeft="30dp">
    </TextView>

    <TextView
        android:id="@+id/create"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="onCreate()"
        android:textColor="@color/black"
        android:textStyle="bold"
        android:textSize="25dp"
        app:layout_constraintTop_toBottomOf="@id/attach"
        app:layout_constraintLeft_toLeftOf="@id/attach"
        android:layout_marginTop="20dp">
    </TextView>

    <TextView
        android:id="@+id/createView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="onCreateView()"
        android:textSize="25dp"
        android:textStyle="bold"
        android:textColor="@color/black"
        app:layout_constraintTop_toBottomOf="@id/create"
        app:layout_constraintLeft_toLeftOf="@id/create"
        android:layout_marginTop="20dp">
    </TextView>

    <TextView
        android:id="@+id/viewCreated"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="onViewCreated()"
        app:layout_constraintTop_toBottomOf="@id/createView"
        app:layout_constraintLeft_toLeftOf="@id/createView"
        android:layout_marginTop="20dp"
        android:textColor="@color/black"
        android:textStyle="bold"
        android:textSize="25dp">
    </TextView>

    <TextView
        android:id="@+id/start"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="onStart()"
        android:textSize="25dp"
        android:textStyle="bold"
        android:textColor="@color/black"
        app:layout_constraintTop_toBottomOf="@id/viewCreated"
        app:layout_constraintLeft_toLeftOf="@id/viewCreated"
        android:layout_marginTop="20dp">
    </TextView>

    <TextView
        android:id="@+id/resume"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="onResume()"
        app:layout_constraintTop_toBottomOf="@id/start"
        app:layout_constraintLeft_toLeftOf="@id/start"
        android:textColor="@color/black"
        android:textStyle="bold"
        android:textSize="25dp"
        android:layout_marginTop="20dp">
    </TextView>

    <TextView
        android:id="@+id/pause"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="onPause()"
        android:textSize="25dp"
        android:textStyle="bold"
        android:textColor="@color/black"
        app:layout_constraintTop_toBottomOf="@id/resume"
        app:layout_constraintLeft_toLeftOf="@id/resume"
        android:layout_marginTop="20dp">
    </TextView>

    <TextView
        android:id="@+id/stop"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="onStop()"
        android:textColor="@color/black"
        android:textStyle="bold"
        android:textSize="25dp"
        app:layout_constraintTop_toBottomOf="@id/pause"
        app:layout_constraintLeft_toLeftOf="@id/pause"
        android:layout_marginTop="20dp">
    </TextView>

    <TextView
        android:id="@+id/destroyView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="onDestroyView()"
        app:layout_constraintTop_toBottomOf="@id/stop"
        app:layout_constraintLeft_toLeftOf="@id/stop"
        android:layout_marginTop="20dp"
        android:textSize="25dp"
        android:textStyle="bold"
        android:textColor="@color/black">
    </TextView>

    <TextView
        android:id="@+id/destroy"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="onDestroy()"
        app:layout_constraintTop_toBottomOf="@id/destroyView"
        app:layout_constraintLeft_toLeftOf="@id/destroyView"
        android:layout_marginTop="20dp"
        android:textColor="@color/black"
        android:textStyle="bold"
        android:textSize="25dp">
    </TextView>

    <TextView
        android:id="@+id/detach"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="onDetach()"
        android:textSize="25dp"
        android:textStyle="bold"
        android:textColor="@color/black"
        app:layout_constraintTop_toBottomOf="@id/destroy"
        app:layout_constraintLeft_toLeftOf="@id/destroy"
        android:layout_marginTop="20dp">
    </TextView>

</androidx.constraintlayout.widget.ConstraintLayout>